\begin{figure}[h]
\centering
\begin{circuitikz} \draw
(1,3) to [short,-o] (0,3) node[below]{+}
(1,3) to [R,l_=$R$,*-*] (3,3)
(1,3) to [short,-] (1,4)
(3,4) to [Do] (1,4)
(3,4) to [short,-] (3,3)
to [short,-*] (4,3)
to [short,-o] (5,3) node[below]{+}
(4,3) to [C, l_=$C$,-*] (4,1)
to [short,-o] (5,1) node[above]{-}
(4,1) to [short,-o] (0,1) node[above]{-}
(0,2) node[]{$v_1$}
(5,2) node[]{$v_2$}
;
\end{circuitikz}
\caption{Et RC-ledd med en diode koblet i parallell med motstanden.} 
\label{fig:M2}
\end{figure}
